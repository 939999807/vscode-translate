{
    "name": "vscode-translate",
    "displayName": "translate",
    "description": "translate others language to Chinese",
    "version": "0.0.1",
    "publisher": "chun",
    "license": "SEE LICENSE IN LICENSE.md",
    "engines": {
        "vscode": "^1.0.0"
    },
    "icon": "img/t.png",
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "contributes": {
        "keybindings": [{
            "command": "extension.translateon",
            "key": "ctrl+t",
            "mac": "cmd+t",
            "when": "editorTextFocus"
        }],
        "commands": [{
            "command": "extension.translate.proxy",
            "title": "translate set proxy"
        }]
    },
    "main": "./out/src/extension",
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    },
    "dependencies": {
        "web-request": "^1.0.3"
    }
}